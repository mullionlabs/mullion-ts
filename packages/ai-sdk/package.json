{
  "name": "@scopestack/ai-sdk",
  "version": "0.0.1",
  "description": "ScopeStack integration with Vercel AI SDK",
  "keywords": [
    "llm",
    "ai-sdk",
    "vercel",
    "typescript",
    "context",
    "openai",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scopestack/scopestack-ts.git",
    "directory": "packages/ai-sdk"
  },
  "license": "MIT",
  "author": "Your Name <your@email.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@scopestack/core": "workspace:*"
  },
  "peerDependencies": {
    "ai": ">=3.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ai-sdk/openai": "catalog:",
    "@types/node": "catalog:",
    "ai": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
