{
  "name": "@mullion/ai-sdk",
  "version": "0.4.0",
  "description": "Mullion integration with Vercel AI SDK",
  "homepage": "https://github.com/mullionlabs/mullion-ts/tree/main/packages/ai-sdk#readme",
  "bugs": {
    "url": "https://github.com/mullionlabs/mullion-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mullionlabs/mullion-ts.git",
    "directory": "packages/ai-sdk"
  },
  "keywords": [
    "mullion",
    "llm",
    "ai",
    "typescript",
    "vercel",
    "ai-sdk",
    "vercel-ai-sdk",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "caching",
    "cost-tracking",
    "observability",
    "opentelemetry",
    "confidence",
    "guardrails",
    "context-management",
    "streaming"
  ],
  "license": "MIT",
  "author": {
    "name": "Oleksandr Huzei",
    "email": "alex.guzey@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src && pnpm run catalog:check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "clean": "rm -rf dist",
    "catalog:normalize": "node ./scripts/normalize-model-catalog.mjs",
    "catalog:check": "node ./scripts/normalize-model-catalog.mjs --check"
  },
  "dependencies": {
    "@mullion/core": "workspace:*"
  },
  "peerDependencies": {
    "ai": ">=3.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ai-sdk/openai": "catalog:",
    "@types/node": "catalog:",
    "ai": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
