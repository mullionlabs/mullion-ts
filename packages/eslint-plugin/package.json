{
  "name": "@mullion/eslint-plugin",
  "version": "0.2.3",
  "description": "ESLint rules for type-safe LLM context management",
  "homepage": "https://github.com/mullionlabs/mullion-ts/tree/main/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/mullionlabs/mullion-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mullionlabs/mullion-ts.git",
    "directory": "packages/eslint-plugin"
  },
  "keywords": [
    "mullion",
    "eslint",
    "eslint-plugin",
    "typescript",
    "lint",
    "static-analysis",
    "security",
    "taint-analysis",
    "dataflow",
    "llm",
    "ai",
    "prompt",
    "context",
    "context-management",
    "guardrails",
    "safety",
    "confidence"
  ],
  "license": "MIT",
  "author": {
    "name": "Oleksandr Huzei",
    "email": "alex.guzey@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@typescript-eslint/utils": "catalog:"
  },
  "peerDependencies": {
    "@mullion/core": ">=0.2.3",
    "@typescript-eslint/parser": ">=8.0.0",
    "eslint": ">=8.57.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@mullion/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mullion/core": "workspace:*",
    "@types/node": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/rule-tester": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
