{
  "name": "@intentkit/core",
  "version": "0.0.1",
  "description": "Core types and utilities for IntentKit",
  "keywords": [
    "llm",
    "typescript",
    "types",
    "context",
    "ai",
    "safety"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/intentkit.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Your Name <your@email.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
